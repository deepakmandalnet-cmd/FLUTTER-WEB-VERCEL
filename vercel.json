{
  "builds": [
    {
      "src": "pubspec.yaml",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "if [ -f \"pubspec.lock\" ]; then rm pubspec.lock; fi && flutter pub get",
        "buildCommand": "flutter build web --release",
        "outputDirectory": "build/web"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
